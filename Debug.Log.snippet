        <Literal Editable="true">
          <Default> </Default>
          <ID>contextclass</ID>
        </Literal>
        <Literal Editable="true">
          <Default> </Default>
          <ID>description</ID>
        </Literal>
        <Literal Editable="true">
          <Default>, this.gameObject </Default>
          <ID>target</ID>
        </Literal>
      </Declarations>
      <Code Language="CSharp">
        <![CDATA[Debug.Log("T($contextclass$): $description$"$target$);
$end$]]>
      </Code>
    </Snippet>
